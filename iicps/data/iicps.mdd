OPTION psf_version = 2.1;

BEGIN driver iicps

  OPTION supported_peripherals = (ps7_i2c);
  OPTION supported_os_types = (DTS);
  OPTION driver_state = ACTIVE;
  OPTION NAME = iicps;
  
  PARAMETER name = bus-id, type = int ;
  PARAMETER name = i2c-clk, type = int , default = 400000;
  PARAMETER name = dts.alias , type = reference, default = i2c;
  PARAMETER name = compatible, default = "cdns,i2c-r1p10", type = stringlist;

END driver
