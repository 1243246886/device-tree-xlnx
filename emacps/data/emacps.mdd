OPTION psf_version = 2.1;

BEGIN driver emacps

  OPTION supported_peripherals = (ps7_ethernet);
  OPTION supported_os_types = (DTS);
  OPTION driver_state = ACTIVE;
  OPTION NAME = emacps;
  PARAMETER name = "#address-cells", default = 1, type = int;
  PARAMETER name = "#size-cells", default = 0, type = int ;
  PARAMETER name = local-mac-address, default = "00 0a 35 00 00 00", type = bytelist
  PARAMETER name = dts.alias, type = string, default = ethernet;
  PARAMETER name = phy-mode, default = gmii
  PARAMETER name = "xlnx,ptp-enet-clock"
END driver
